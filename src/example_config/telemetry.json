{
  "telemetry": {
    "enabled": true,
    "service_name": "overlaybd",
    "service_version": "1.0.0",
    "exporters": {
      "otlp": {
        "enabled": true,
        "endpoint": "http://localhost:4318/v1/traces",
        "timeout_ms": 5000,
        "headers": {}
      },
      "jaeger": {
        "enabled": false,
        "endpoint": "http://localhost:14268/api/traces",
        "timeout_ms": 5000
      }
    },
    "sampling": {
      "type": "always_on",
      "rate": 1.0
    },
    "batch_processor": {
      "max_queue_size": 2048,
      "schedule_delay_ms": 500,
      "max_export_batch_size": 512
    }
  }
}